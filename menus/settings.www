<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.0//EN">

<html>
  <head>
    <title>$b</title>
    <meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
    <link rel="stylesheet" href="bbbs.css" type="text/css">
  </head>
  <body>
    <table frame="border" cellpadding="5" width="640" border="1" cellspacing="0" class="header">
      <tr><td class="logo">$u
    </table>

    <p>
      [<a href="bbbs?id=$i;do=l">Logout</a>]
      [<a href="bbbs?id=$i;do=o">Who's Online</a>]
      [<a href="bbbs?id=$i;do=">Message Areas</a>]
      [<a href="bbbs?id=$i;do=l%2f,21">File Areas</a>]
    </p>

    <form action="bbbs?id=$i;do=S" method="post" onSubmit="window.status='Sending changes'; return true">
      <table border="1" cellspacing="0" cellpadding="5">
        <tr class="header">
          <td><strong>Settings</strong></td>
        </tr>
        <tr>
          <td>
            <table class="data" cellspacing="1" border="0" cellpadding="5">
              <tr>
                <td align="right"><strong>New password:</strong></td>
                <td><input type=password name="passwd1" size=29 maxlength=70></td>
              </tr>
              <tr>
                <td align="right"><strong>New password:</strong></td>
                <td><input type=password name="passwd2" size=29 maxlength=70></td>
              </tr>
              <tr>
                <td align="right"><strong>Street address:</strong></td>
                <td><input type=text name="straddr" value="$S1" size=29></td>
              </tr>
              <tr>
                <td align="right"><strong>Postal code and state:</strong></td>
                <td><input type=text name="postal" value="$S2" size=29></td>
              </tr>
              <tr>
                <td align="right"><strong>Telephone number:</strong></td>
                <td><input type=text name="tele" value="$S3" size=17></td>
              </tr>
              <tr>
                <td align="right"><strong>Date of birth:</strong></td>
                <td><input type=text name="dob" value="$S4" size=17></td>
              </tr>
              <tr>
                <td align="right"><strong>Language:</strong></td>
                <td><select name="lang">$S5</select></td>
              </tr>
              <tr>
                <td align="right"><strong>Charset:</strong></td>
                <td><select name="charset">$S7</select></td>
              </tr>
              <tr>
                <td align="right"><strong>Grab format:</strong></td>
                <td><select name="grabform">$S6</select></td>
              </tr>
              <tr>
                <td>&nbsp;</td>
                <td><input type=submit value="Send changes"><input type=reset value="Reset values"></td>
              </tr>
            </table>
          </td>
        </tr>
      </table>
    </form>
  </body>
</html>
